    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera.ar" android:required="true"/>
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />
    <application
        android:allowBackup="true"
        android:name=".ApplicationContext"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MetacoGroup"
        android:usesCleartextTraffic="false"
        tools:targetApi="31">
        <activity android:name=".NursePanelSmsVerificationCode"
            android:theme="@style/Theme.MetacoGroup_noaktionbar"
            android:exported="true">

        </activity>
        <activity android:name=".NursepanelSignup"
            android:theme="@style/Theme.MetacoGroup_noaktionbar"
            android:windowSoftInputMode="adjustPan"
            android:exported="false">

        </activity>
        <activity android:name=".FaceActivity"
            android:exported="true"
            android:theme="@style/Theme.MetacoGroup_noaktionbar"
            >

        </activity>
        <activity android:name=".editProfile"
            android:exported="true">

        </activity>
        <activity android:name=".emtiazdehi"
            android:theme="@style/Theme.MetacoGroup_noaktionbar"
            android:exported="false">

        </activity>
        <activity android:name=".NursePanelLogin"
            android:exported="true"
            android:theme="@style/Theme.MetacoGroup_noaktionbar"
            >

        </activity>
        <activity
            android:name=".NursePanelMenu"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".NursePanelRecycle"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MetacoGroup_noaktionbar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <meta-data android:name="com.google.ar.core" android:value="required" />
    </application>

</manifest>

